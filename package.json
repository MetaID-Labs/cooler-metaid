  "devDependencies": {
    "@vitest/ui": "^0.34.1",
    "path": "^0.12.7",
    "prettier": "^3.0.3",
    "typescript": "^5.1.6",
    "vite": "^4.4.11",
    "vite-plugin-dts": "^3.6.0",
    "vitest": "^0.34.6"
  },
  "dependencies": {
